@*<canvas id="myChart"></canvas>*@
<canvas id="myChart"></canvas>
<script>
        //Will JSON Information assigned to JavaScript array
        //Month (1 month~6 month)
        var jsTimeDate = @Html.Raw(ViewBag.jsonTimeDailyCuElectroless);

        //Three cities City,Business
        var jsH = @Html.Raw(ViewBag.jsonHCuElectroless);
        var jsM = @Html.Raw(ViewBag.jsonMCuElectroless);
        var jsL = @Html.Raw(ViewBag.jsonLCuElectroless);


        var ctx = document.getElementById("myChart");
        var chart = new Chart(ctx, {
            type: "line",
            data: {
                //labels: ['1 month', '2 month', '3 month', '4 month', '5 month', '6 month'],
                labels: jsTimeDate,
                datasets: [{
                    label: "Action H",
                    //label: jsBusiness[0].City,

                    fill: false,
                    backgroundColor: 'rgba(255,165,0,0.3)',
                    borderColor: 'rgb(255,135,20)',
                    pointStyle: "circle",
                    pointBackgroundColor: 'rgb(0,222,0)',
                    pointRadius: 5,
                    pointHoverRadius: 10,
                    //data: jsBusiness[0].Business
                    //data: [13.1, 10.2, 13.5, 20.9, 25.2, 27.1, 31.8]
                    data: jsH
                }, {
                    label: "Action M",
                    //label: jsBusiness[1].City,
                    fill: false,
                    backgroundColor: 'rgba(0,255,255,0.3)',
                    borderColor: 'rgb(0,225,255)',
                    pointStyle: "triangle",
                    pointBackgroundColor: 'blue',
                    pointRadius: 5,
                    pointHoverRadius: 10,
                    //data: jsBusiness[1].Business
                    //data: [29.1, 28.3, 22.6, 25.4, 27.5, 23.4]
                    data: jsM

                }, {
                    label: "Action L",
                    //label: jsBusiness[2].City,
                    fill: false,
                    backgroundColor: 'rgba(153,50,204,0.3)',
                    borderColor: 'rgb(123,55,190)',
                    pointStyle: "rect",
                    pointBackgroundColor: 'rgb(220,20,60)',
                    pointRadius: 5,
                    pointHoverRadius: 10,
                    //data: jsBusiness[2].Business
                    //data: [16.6, 17.3, 19.2, 23.8, 12.0, 17.6]
                    data: jsL
                }]
            },
            options: {
                responsive: true,
                title: {
                    display: true,
                    fontSize: 25,
                    text: 'Analysis Result line : PTH 4 Cu Electroless'
                },
                tooltips: {
                    mode: 'point',
                    intersect: true,
                },
                hover: {
                    mode: 'nearest',
                    intersect: true
                },
                scales: {
                    xAxes: [{
                        display: true,
                        scaleLabel: {
                            display: true,
                            labelString: 'Monthly',
                            fontSize: 15
                        },
                        ticks: {
                            fontSize: 10
                        }
                    }],
                    yAxes: [{
                        display: true,
                        scaleLabel: {
                            display: true,
                            labelString: '2.0-2.8 g/L',
                            fontSize: 12
                        },
                        ticks: {
                            fontSize: 10,
                            max: 3.00,
                            min: 1.80,
                            stepSize: 0.20

                        }
                    }]
                },
                animation: {
                    duration: 2000
                }
            }
        });

</script>
