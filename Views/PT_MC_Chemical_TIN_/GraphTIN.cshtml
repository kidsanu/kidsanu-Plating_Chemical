}


<form>
    <div class="row">
        <div class="col-sm-4">
            <div class="form-group">
                @*@Html.Label("Chemical Name :", htmlAttributes: new { @class = "control-label col-md-8" })*@
                <div class="col-md-10">
                    @*@Html.DropDownListFor(m => m.PART_NO, selectList, "--Select Chemical--", new { @class = "css-class" })*@
                </div>
            </div>
        </div>

        <div class="col-sm-4">
            <div class="form-group">


                @Html.LabelFor(model => model.START_DATED, htmlAttributes: new { @class = "control-label col-md-8" })
                <div class="col-md-10">
                    @Html.TextBoxFor(model => model.START_DATED, new { type = "month" })
                    @Html.ValidationMessageFor(model => model.START_DATED, "", new { @class = "text-danger" })
                </div>
            </div>
        </div>


        <div class="col-sm-4">
            <div class="form-group">
                <div class="col-md-10">
